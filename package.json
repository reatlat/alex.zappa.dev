{
	"name": "alex.zappa.dev",
	"version": "6.0.0",
	"description": "The absolutely delicious, I mean really just scrumptious, personal site for Alex Zappa.",
	"main": ".eleventy.js",
	"scripts": {
		"clean:site": "rimraf './_site'",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
		"postbuild": "npm run export:twconfig",
		"view:twconfig": "npx tailwind-config-viewer -o",
		"export:twconfig": "npx tailwind-config-viewer export ./_site/tw-config",
		"debug:11ty": "DEBUG=Eleventy* npx @11ty/eleventy",
		"watch:11ty": "npx @11ty/eleventy --serve --quiet",
		"build:11ty": "npx @11ty/eleventy",
		"build:js": "esbuild ./src/assets/js/main.js --outfile=./_site/js/main.js --bundle --minify",
		"watch:js": "esbuild ./src/assets/js/main.js --outfile=./_site/js/main.js --bundle --watch --sourcemap",
		"watch:tailwind": "npx tailwindcss --postcss -i src/assets/css/main.css -o ./_site/css/main.css --watch",
		"build:tailwind": "npx tailwindcss --postcss -i src/assets/css/main.css -o ./_site/css/main.css --minify",
		"start": "ELEVENTY_ENV=development npm-run-all --parallel watch:*",
		"build": "ELEVENTY_ENV=production npm-run-all --parallel build:*",
		"debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
		"prettier:check": "prettier --check './src'",
		"prettier:fix": "prettier --write .",
		"prepare": "husky install",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/reatlat/alex.zappa.dev.git"
	},
	"keywords": [
		"alex",
		"zappa",
		"11ty",
		"eleventy",
		"tailwindcss"
	],
	"author": {
		"name": "Alex Zappa",
		"email": "alex@zappa.dev",
		"url": "https://alex.zappa.dev/"
	},
	"license": "UNLICENSED",
	"private": true,
	"bugs": {
		"url": "https://github.com/reatlat/alex.zappa.dev/issues"
	},
	"homepage": "https://github.com/reatlat/alex.zappa.dev#readme",
	"devDependencies": {
		"@11ty/eleventy": "^2.0.1",
		"@11ty/eleventy-img": "^3.1.0",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"@fullhuman/postcss-purgecss": "^5.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/line-clamp": "^0.4.4",
		"@tailwindcss/typography": "^0.5.9",
		"alpinejs": "^3.12.2",
		"autoprefixer": "^10.4.14",
		"daisyui": "^3.0.2",
		"eleventy-plugin-feathericons": "^1.0.1",
		"esbuild": "^0.17.19",
		"eslint-config-prettier": "^8.8.0",
		"flying-pages-module": "^2.1.3",
		"husky": "^8.0.3",
		"luxon": "^3.3.0",
		"markdown-it-anchor": "^8.6.7",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.24",
		"postcss-cli": "^10.1.0",
		"postcss-discard-comments": "^6.0.0",
		"postcss-easy-import": "^4.0.0",
		"postcss-merge-rules": "^6.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"pretty-quick": "^3.1.3",
		"prismjs": "^1.29.0",
		"rimraf": "^5.0.1",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "^9.0.5",
		"tailwind-config-viewer": "^1.7.2",
		"tailwindcss": "^3.3.2",
		"tailwindcss-safe-area": "^0.2.2"
	}
}
