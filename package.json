{
  "name": "alex.zappa.dev",
  "version": "7.0.0",
  "description": "The absolutely delicious, I mean really just scrumptious, personal site for Alex Zappa.",
  "main": "eleventy.config.js",
  "type": "module",
  "scripts": {
    "clean:site": "rimraf './_site'",
    "blog:new": "node scripts/new-post.js",
    "postbuild": "echo 'Postbuild complete!'",
    "postbuild:og-images": "node scripts/generate-og-images.js",
    "debug:11ty": "DEBUG=Eleventy* npx @11ty/eleventy",
    "watch:11ty": "npx @11ty/eleventy --serve --quiet",
    "build:11ty": "npx @11ty/eleventy",
    "watch:js": "npx esbuild ./src/assets/js/main.js --outfile=./_site/js/main.js --bundle --watch --sourcemap",
    "build:js": "npx esbuild ./src/assets/js/main.js --outfile=./_site/js/main.$(git rev-parse --short HEAD).js --bundle --minify",
    "watch:tailwind": "npx tailwindcss --postcss -i src/assets/css/main.css -o ./_site/css/main.css --watch",
    "build:tailwind": "npx tailwindcss --postcss -i src/assets/css/main.css -o ./_site/css/main.$(git rev-parse --short HEAD).css --minify",
    "start": "ELEVENTY_ENV=development npm-run-all --parallel watch:*",
    "build": "ELEVENTY_ENV=production npm-run-all --serial build:js build:tailwind build:11ty",
    "debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
    "benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "prettier:check": "npx prettier --check './src'",
    "prettier:fix": "npx prettier --write .",
    "prepare": "husky",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/reatlat/alex.zappa.dev.git"
  },
  "keywords": [
    "alex",
    "zappa",
    "11ty",
    "eleventy",
    "tailwindcss"
  ],
  "author": {
    "name": "Alex Zappa",
    "email": "alex@zappa.dev",
    "url": "https://alex.zappa.dev/"
  },
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/reatlat/alex.zappa.dev/issues"
  },
  "homepage": "https://github.com/reatlat/alex.zappa.dev#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "6.0.1",
    "@11ty/eleventy-plugin-rss": "^2.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@alpinejs/intersect": "^3.14.8",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "alpinejs": "^3.14.8",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.4.1",
    "dracula-prism": "^2.1.16",
    "eleventy-plugin-phosphoricons": "^1.3.0",
    "eleventy-plugin-speculation-rules": "^1.1.0",
    "esbuild": "^0.24.2",
    "eslint-config-prettier": "^10.0.1",
    "flying-pages-module": "^2.1.4",
    "html-minifier-terser": "^7.2.0",
    "husky": "^9.1.7",
    "inquirer": "^12.3.3",
    "isbot": "^5.1.22",
    "js-yaml": "^4.1.0",
    "lite-youtube-embed": "^0.3.3",
    "luxon": "^3.5.0",
    "markdown-it-anchor": "^9.2.0",
    "netlify-plugin-11ty": "^1.4.0",
    "node-html-to-image": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "plausible-tracker": "^0.3.9",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "postcss-discard-comments": "^7.0.3",
    "postcss-easy-import": "^4.0.0",
    "postcss-merge-rules": "^7.0.4",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "pretty-quick": "^4.0.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "tailwindcss": "~3.4.17",
    "tailwindcss-safe-area": "^0.6.0",
    "uuid": "^11.0.5"
  }
}
